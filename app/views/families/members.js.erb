<%= raw "document.write('"%> <% @families.each do |c| %> <%= image_tag(c.photo_url(:thumb), :id => 'photo', :alt => 'photo', :style => 'float:right;')%><div style="font-size:larger;"><%=c.husband_name%> & <%=c.wife_name%> <strong><%=c.last_name%></strong></div><a target="_blank" href="<%='http://maps.google.com/maps?q=' + c.address.gsub(' ', '+') + '+' + c.city_state_zip.gsub(' ', '+')%>"><%=c.address%>, <%=c.city_state_zip%></a><%= " (Phone: " + c.home_phone + ")" if c.home_phone? %><br /><div style="margin-left:15px;"><%=c.husband_name%> - <a href="mailto:<%=c.husband_email%>"><%=c.husband_email%></a><%= " / " + c.husband_phone if c.husband_phone? %> <br /><%=c.wife_name%> - <a href="mailto:<%=c.wife_email%>"><%=c.wife_email%></a> <%= " / " + c.wife_phone if c.wife_phone? %> </div><hr style="clear:both;" /><% end %><%= raw "');" %>